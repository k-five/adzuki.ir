{
  "name": "adzuki",
  "version": "1.0.0",
  "description": "Setup and Deploy an SPA in less than 1 minute.",
  "main": "",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "multer": "^1.4.1",
    "node-dev": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "mocha": "^6.1.4",
    "postcss-cli": "^6.1.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  },
  "scripts": {
    "start": "node-dev adzuki.js",
    "webpack": "./node_modules/.bin/webpack",
    "webpack.watch": "./node_modules/.bin/webpack -w",
